{
  "name": "andore-ui",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-all": "pnpm run -r build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "pnpm --filter @andore-ui/stories storybook",
    "website:build": "pnpm --filter ./apps/www build",
    "website:export": "pnpm --filter ./apps/www export",
    "packages:build": "pnpm --filter ./packages/* build",
    "packages:publish": "pnpm --filter ./packages/* publish --access public",
    "storybook:build": "pnpm --filter @andore-ui/stories run build",
    "create:package:radix": "node scripts/add-radix-package.js",
    "create:component:radix": "node scripts/add-radix-component.js",
    "create:component": "node scripts/create-new-component.js",
    "create:package": "node scripts/create-new-package.js",
    "create:story": "node scripts/create-story.js"
  },
  "dependencies": {
  },
  "devDependencies": {
  }
}
